{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The Root Schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "additionalProperties": true,
    "properties": {
        "permissionSets": {
            "$id": "#/properties/permissionSets",
            "type": "object",
            "description": "Permissionsets patches",
            "default": {},
            "additionalProperties": true,
            "properties": {
                "stripUselessFls": {
                    "$id": "#/properties/permissionSets/properties/stripUselessFls",
                    "type": "boolean",
                    "title": "The Stripuselessfls Schema",
                    "description": "if stripUselessFls is true, fieldPermissions in which both readable and editable tags are false are removed from the XML. They are totally redundand since a PermissionSet can only add permissions.",
                    "default": true
                }
            }
        },
        "objectTranslations": {
            "$id": "#/properties/objectTranslations",
            "type": "object",
            "description": "objectTranslations patches",
            "default": {},
            "additionalProperties": true,
            "properties": {
                "stripUntranslatedFields": {
                    "$id": "#/properties/objectTranslations/properties/stripUntranslatedFields",
                    "type": "boolean",
                    "description": "if stripUntranslatedFields is true, untranslated tags are removed from the XML.",
                    "default": true
                },
                "stripNotVersionedFields": {
                    "$id": "#/properties/objectTranslations/properties/stripNotVersionedFields",
                    "type": "boolean",
                    "description": "if stripNotVersionedFields is true, translated fields that are not present in the file system in the corresponding .object files, are removed from the XML",
                    "default": true
                }
            }
        },
        "preDeployPlugins": {
            "$id": "#/properties/preDeployPlugins",
            "type": "array",
            "title": "The Predeployplugins Schema",
